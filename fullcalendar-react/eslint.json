{
  "extends": [ "eslint-config-standard", "plugin:react/recommended" ],
  "parser": "babel-eslint",
  "plugins": [
    "jest"
  ],
  "env": {
    "jest/globals": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "space-before-function-paren": 0,
    "no-multiple-empty-lines": 0
  }
}
